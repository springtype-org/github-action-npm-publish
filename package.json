{
  "name": "github-action-npm-publish",
  "version": "1.0.1",
  "private": true,
  "description": "Publish an node repository to npm.",
  "main": "dist/index.cjs.js",
  "engines": {
    "node": ">=16.0"
  },
  "scripts": {
    "prebuild": "yarn node ./prebuild.mjs",
    "build": "yarn node --enable-source-maps ./build.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/springtype-org/github-action-npm-publish.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Michael Mannseicher",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@jsheaven/easybuild": "^1.2.9",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/semver": "^7.5.8",
    "typescript": "^5.4.5"
  }
}
